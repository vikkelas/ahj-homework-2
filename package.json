{
  "name": "Home-work",
  "version": "1.0.0",
  "description": "Game",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "test": "jest --testPathPattern=./src/ --coverage",
    "coverage": "jest --testPathPattern=./src/ --coverage",
    "doc": "jsdoc -c jsdoc.conf.json",
    "preshow:coverage": "yarn run coverage",
    "show:coverage": "live-server coverage/lcov-report",
    "preshow:doc": "yarn run doc",
    "show:doc": "live-server doc",
    "preshow:dist": "yarn run build",
    "show:dist": "live-server dist"
  },
  "author": "Vikkelas",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "css-loader": "^3.6.0",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "terser-webpack-plugin": "^1.4.5",
    "webpack": "^4.44.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2",
    "yarn": "^1.22.10"
  }
}
